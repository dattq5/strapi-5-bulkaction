{
  "extends": "@strapi/typescript-utils/tsconfigs/server",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "resolveJsonModule": true
  },
  "include": ["./", "./**/*.ts", "./**/*.tsx", "./**/*.js", "src/**/*.json", "custom.d.ts"],
  "exclude": [
    "node_modules/",
    "build/",
    "dist/",
    ".cache/",
    ".tmp/",
    "src/admin/",
    "**/*.test.*",
    "src/plugins/**",
    "**/generated/**",
    "coverage/",
  ]
}
